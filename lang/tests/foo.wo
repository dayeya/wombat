fn free putcharln(out: ch)
    putchar(out);
    putchar('\n');
end

fn free putcharws(out: ch)
    putchar(out);
    putchar(' ');
end

fn free main()
    putchar('h');
    putchar('e');
    putchar('l');
    putchar('l');
    putcharws('o');

    putchar('w');
    putchar('o');
    putchar('r');
    putchar('l');
    putcharln('d');
end